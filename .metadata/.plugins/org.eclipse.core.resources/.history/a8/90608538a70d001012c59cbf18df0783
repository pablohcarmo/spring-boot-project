package br.com.sosysters.pojo;

public class Provider {
	private int idProvider;
	private String professionalQualification;
	private Integer rateStars;
	private Integer rateComments;
	private String payment;

	public Provider() {
		super();
		// TODO Auto-generated constructor stub
	}

	public Provider(int idProvider, String professionalQualification, Integer rateStars, Integer rateComments, String payment) {
		super();
		this.idProvider = idProvider;
		this.professionalQualification = professionalQualification;
		this.rateStars = rateStars;
		this.rateComments = rateComments;
		this.payment = payment;
	}




	public int getIdProvider() {
		return idProvider;
	}
	public void setIdProvider(int idProvider) {
		this.idProvider = idProvider;
	}
	public String getProfessionalQualification() {
		return professionalQualification;
	}
	public void setProfessionalQualification(String professionalQualification) {
		this.professionalQualification = professionalQualification;
	}
	public Integer getRateStars() {
		return rateStars;
	}
	public void setRateStars(Integer rateStars) {
		this.rateStars = rateStars;
	}
	public Integer getRateComments() {
		return rateComments;
	}
	public void setRateComments(Integer rateComments) {
		this.rateComments = rateComments;
	}
	public String getPayment() {
		return payment;
	}
	public void setPayment(String payment) {
		this.payment = payment;
	}


}
